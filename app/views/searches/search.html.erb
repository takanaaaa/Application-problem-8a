<div class="container">
  <div class="row">
    <div class="col-10">
      <% if @range == "1" %>
        <h2>User search for <%= @word　%></h2>
        <%= render "users/index", users: @user %>
      <% else %>
        <h2>Books search for <%= @word　%></h2>
        <%= render "books/index", books: @book %>
      <% end %>
    </div>
  </div>
</div>
